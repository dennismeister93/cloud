{
  "name": "kilocode-backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:prod-db": "USE_PRODUCTION_DB=true next dev --turbo",
    "build": "next build",
    "start": "next start",
    "stripe": "stripe listen --forward-to http://localhost:3000/api/stripe/webhook",
    "lint": "NODE_OPTIONS='--max-old-space-size=8192' eslint --config eslint.config.mjs --cache 'src/**/*.{ts,tsx,js,jsx}' 'cloudflare-ai-attribution/src/**/*.{ts,tsx,js,jsx}' 'cloudflare-deploy-infra/**/src/**/*.{ts,tsx,js,jsx}'",
    "lint:changed": "NODE_OPTIONS='--max-old-space-size=8192' git diff --name-only $(git merge-base origin/main HEAD) --diff-filter=ACMR -- '**/*.js' '**/*.jsx' '**/*.ts' '**/*.tsx' | NODE_OPTIONS='--max-old-space-size=8192' xargs -r eslint --fix --config eslint.config.mjs --cache",
    "lint-staged": "NODE_OPTIONS='--max-old-space-size=8192' lint-staged",
    "format": "prettier --write --list-different \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:changed": "git diff --name-only $(git merge-base origin/main HEAD) --diff-filter=ACMR -- '**/*.js' '**/*.jsx' '**/*.ts' '**/*.tsx' '**/*.json' '**/*.css' '**/*.scss' '**/*.md' | xargs -r prettier --write --list-different",
    "test": "NODE_ENV=test jest",
    "typecheck-oldtsc": "tsc --noEmit",
    "typecheck": "tsgo --noEmit --incremental false",
    "dependency-cycle-check": "madge --circular --warning --extensions ts,tsx --ts-config tsconfig.json --basedir . src",
    "validate": "pnpm run typecheck && pnpm run lint:changed && pnpm run format:changed && pnpm run test && pnpm run dependency-cycle-check",
    "prepare": "sh -c '[ \"$HUSKY\" = 0 ] && git config --unset core.hooksPath || husky'",
    "preinstall": "npx only-allow pnpm",
    "node": "node",
    "script": "IS_SCRIPT=true tsx --tsconfig tsconfig.scripts.json",
    "script:run": "tsx --tsconfig tsconfig.scripts.json src/scripts/index",
    "db:empty-database": "tsx --tsconfig tsconfig.scripts.json src/db/empty-database.ts",
    "drizzle": "drizzle-kit",
    "knip": "knip",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "promo": "pnpm -s script src/scripts/encrypt-promo-codes.ts"
  },
